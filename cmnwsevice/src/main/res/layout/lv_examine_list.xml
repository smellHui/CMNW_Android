<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data></data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <LinearLayout style="@style/hor_ll.action">

            <TextView
                style="@style/text.actionTitle"
                android:text="审核人" />

            <TextView
                android:id="@+id/tv_worker_name"
                style="@style/text.actionContent"
                android:text="暂无" />
        </LinearLayout>

        <LinearLayout style="@style/hor_ll.action">

            <TextView
                style="@style/text.actionTitle"
                android:text="审核时间" />

            <TextView
                android:id="@+id/tv_worker_time"
                style="@style/text.actionContent"
                android:text="暂无" />
        </LinearLayout>

        <LinearLayout style="@style/hor_ll.action">

            <TextView
                style="@style/text.actionTitle"
                android:text="审核意见" />

            <TextView
                android:id="@+id/tv_worker_type"
                style="@style/text.actionContent"
                android:text="暂无" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lo_des"
            style="@style/hor_ll.action">

            <TextView
                style="@style/text.actionTitle"
                android:text="退回原因" />

            <TextView
                android:id="@+id/tv_worker_des"
                style="@style/text.actionContent"
                android:text="暂无" />
        </LinearLayout>
    </LinearLayout>
</layout>
