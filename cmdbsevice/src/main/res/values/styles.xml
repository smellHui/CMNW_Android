<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--乡镇统计-->
    <style name="text.statics">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color_4FB97E</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="text.title">
        <item name="android:width">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/color_8993A9</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="text.item_spss">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/color_434952</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="text.event_pop">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingLeft">9dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:textColor">@color/color_666666</item>
    </style>

    <style name="text.bold_16">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/color_333333</item>
    </style>

    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#e8e8e8</item>
    </style>

    <style name="alarm_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:drawableEnd">@drawable/checkbox_style</item>
        <item name="android:drawableRight">@drawable/checkbox_style</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:paddingBottom">50dp</item>
    </style>

    <style name="radio_btn_tag">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@drawable/datecate_text_color</item>
        <item name="android:background">@drawable/datecate_tag_bg</item>
    </style>

    <style name="ll_statis">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="img_statis">
        <item name="android:layout_height">41dp</item>
        <item name="android:layout_width">41dp</item>
    </style>

    <style name="tv_statis">
        <item name="android:drawablePadding">8dp</item>
        <item name="android:drawableRight">@mipmap/next</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="text.count_statis">
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="tag_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">15dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="edittext_pick_date">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/shape_corner_et</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColorHint">#BBBBBB</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="text.15_000000">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text.supervised_time">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#7D8594</item>
    </style>

    <style name="text.supervised_content">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#475162</item>
    </style>

    <style name="text.12_07090D">
        <item name="android:textColor">#07090D</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text.12_000000">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text.12_333333">
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text.12_475162">
        <item name="android:textColor">#475162</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text.12_FEAA18">
        <item name="android:textColor">#FEAA18</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="btn.supervised">
        <item name="android:layout_height">27dp</item>
        <item name="android:layout_width">90dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_corner</item>
    </style>
</resources>